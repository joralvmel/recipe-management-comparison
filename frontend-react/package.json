{
  "name": "frontend-react",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "serve:build": "npm run build && npm run preview",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "vitest run --coverage && node -e \"import('open').then(open => open.default('./coverage/index.html'))\"",
    "lighthouse": "npm run build && tsx src/utils/lighthouse-script.ts",
    "analyze": "vite build --sourcemap && npx source-map-explorer dist/assets/*.js --json > dist/bundle-analysis.json",
    "sonar:branch": "tsx sonar.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.1",
    "@tanstack/react-query": "^5.74.4",
    "axios": "^1.8.2",
    "jwt-decode": "^4.0.0",
    "lighthouse": "^12.5.1",
    "puppeteer": "^24.7.1",
    "react-router-dom": "^7.3.0",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.1.1",
    "dotenv": "^16.5.0",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "open": "^10.0.4",
    "react-devtools": "^6.1.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sonarqube-scanner": "^4.3.0",
    "source-map-explorer": "^2.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "web-vitals": "^5.0.0-rc.1"
  }
}
